{
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }],
    "github": {
        "silent": true
    }
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
}
  
